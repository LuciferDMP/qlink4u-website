Options -MultiViews
RewriteEngine On

# Handle specific system routes
RewriteRule ^dashboard$ /index.html [L]
RewriteRule ^profile$ /index.html [L]
RewriteRule ^settings$ /index.html [L]
RewriteRule ^analytics/(.*)$ /index.html [L]

# Handle Angular and other front-end routes
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.html [L]